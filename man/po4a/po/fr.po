msgstr "defoma_font_unregister( I<catégorie>, I<police> );"

# type: Plain text
#: ../Defoma::Font.3pm:24
msgid "defoma_font_reregister( I<category>, I<font>, I<hints...> );"
msgstr "defoma_font_reregister( I<catégorie>, I<police>, I<indications...> );"

# type: Plain text
#: ../Defoma::Font.3pm:29
msgid "defoma_font_if_register( I<category>, I<font> );"
